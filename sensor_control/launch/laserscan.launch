<launch>

  <!--velo to laserscan-->
  <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan_node" args="cloud_in:=/velodyne_points" respawn="false">
    <param name="range_min" value="1.5"/>
    <param name="range_max" value="100"/>
    <param name="min_height" value="-0.3"/>
    <param name="max_height" value="-0.05"/>
<param name="angle_increment" value="0.0021816615649929098"/>
<param name="concurrency_level" value="3"/>
  </node>
  <node pkg="tf" type="static_transform_publisher" name="static_tf_laser_to_velodyne" args="0 0 0.4 0 0 0 base_link velodyne 100"/>

</launch>
